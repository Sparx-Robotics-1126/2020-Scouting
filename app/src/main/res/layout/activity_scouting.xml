                    android:layout_height="wrap_content"
                    android:text="Tried to level"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/successfullyLeveledRadioButton"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Successfully leveled"
                    android:textSize="20dp">
                </RadioButton>
            </RadioGroup>
        </LinearLayout>
        <TextView
            android:id="@+id/otherTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="40dp"
            android:text="Other:">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/defenseTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Defense:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/excellecentDefenseTextView"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Played Excellent Defense"
                        android:textSize="22dp">
                    </TextView>

                    <CheckBox android:id="@+id/excellecentDefenseCheckBox"
                        style="@style/t019_checkBox"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/foulTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Foul:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/causedFouldTextView"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Caused a foul"
                        android:textSize="22dp">
                    </TextView>

                    <CheckBox android:id="@+id/causedFouldCheckBox"
                        style="@style/t019_checkBox"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/robotStateTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Robot State:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/brokeDisabledTextView"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Broke or got Disabled"
                        android:textSize="22dp">
                    </TextView>

                    <CheckBox android:id="@+id/brokeDisabledCheckBox"
                        style="@style/t019_checkBox"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <Button
            android:id="@+id/saveButton"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_marginTop="275dp"
            android:shadowColor="#673AB7"
            android:text="Save"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="#2196F3"/>
    </LinearLayout>
</ScrollView>