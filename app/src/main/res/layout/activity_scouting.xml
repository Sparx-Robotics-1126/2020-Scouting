<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/backgroundScrollView">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/assignmentTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Assignment:"
            android:textSize="40dp">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/scouterNameTextView"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="Scouter:"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="22dp">
            </TextView>
            <TextView
                android:id="@+id/scouterNameInput"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Large">
            </TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/matchNumTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Match Number:"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="22dp">
            </TextView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="right">
                <Button
                    android:id="@+id/plusMatch"
                    style="@style/t019_plusMinusButton"
                    android:text="+" />
                <TextView
                    android:id="@+id/matchNumInput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/minusMatch"
                    style="@style/t019_plusMinusButton"
                    android:layout_weight="0"
                    android:text="-" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/logOutButton"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:shadowColor="#673AB7"
                android:text="Log Out"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#2196F3" >
            </Button>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".5">
                <TextView
                    android:id="@+id/teamNumTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Team "
                    android:textSize="22dp"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large">
                </TextView>
                <TextView
                    android:id="@+id/teamNumInput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="22dp"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large">
                </TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".5">
                <TextView
                    android:id="@+id/allianceTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="10dp"
                    android:text="Alliance Color"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <TextView
                    android:id="@+id/allianceInput"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/autonomousTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="40dp"
            android:text="Autonomous:">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/startingPosTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Starting Position:"
                android:textSize="25dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Large">
            </TextView>
            <RadioGroup
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/startingPositionRadioGroup">
                <RadioButton
                    android:id="@+id/closestRadioButton"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Closest to Power Port"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/middleRadioButton"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Middle"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/farthestRadioButton"
                    android:layout_width="match_parent"
                    style="@style/t019_radioButton"
                    android:layout_height="wrap_content"
                    android:text="Farthest to Power Port"
                    android:textSize="20dp">
                </RadioButton>
            </RadioGroup>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/startBallsTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Power Cells in robot at start:"
                    android:textSize ="25dp">
                </TextView>
                <RadioGroup
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/startingBallsRadioGroup">
                    <RadioButton
                        android:id="@+id/noneRadioButton"
                        style="@style/t019_radioButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="None"
                        android:textSize="20dp">
                    </RadioButton>
                    <RadioButton
                        android:id="@+id/oneRadioButton"
                        style="@style/t019_radioButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="One"
                        android:textSize="20dp">
                    </RadioButton>
                    <RadioButton
                        android:id="@+id/twoRadioButton"
                        style="@style/t019_radioButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Two"
                        android:textSize="20dp">
                    </RadioButton>
                    <RadioButton
                        android:id="@+id/threeRadioButton"
                        style="@style/t019_radioButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Three"
                        android:textSize="20dp">
                    </RadioButton>
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ballsScoredAutoTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Power Cells Scored:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/bottomPortAutoTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bottom port"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsBottomAuto"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/ballsBottomAutoInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />
                    <Button
                        android:id="@+id/minusBallsBottomAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/outerPortAutoTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Outer port"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsOuterAuto"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/ballsOuterAutoInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/minusBallsOuterAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/innerPortAutoTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Inner port"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsInnerAuto"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/ballsInnerAutoInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/minusBallsInnerAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ballsAcquiredAutoTextView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Power Cells Acquired:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/floorAutoTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Floor"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsFloorAuto"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/ballsFloorAutoInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/minusBallsFloorAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/crossesLineTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Cross Initiation Line:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/crossedLineTextView"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Left Line at some point"
                        android:textSize="22dp">
                    </TextView>

                    <CheckBox
                        android:id="@+id/crossedLineCheckBox"
                        style="@style/t019_checkBox"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/teleoperatedTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teleoperated:"
            android:textSize="40dp">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/ballsScoredTeleTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Power Cells Scored:"
                android:textSize="25dp">
            </TextView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/bottomPortTeleTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Bottom port"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="right">
                <Button
                    android:id="@+id/plusBallsBottomTele"
                    style="@style/t019_plusMinusButton"
                    android:text="+" />
                <TextView
                    android:id="@+id/ballsBottomTeleInput"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold" />
                <Button
                    android:id="@+id/minusBallsBottomTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_weight="0"
                    android:text="-" />
            </LinearLayout>
            </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/outerPortTeleTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Outer port"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsOuterTele"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/ballsOuterTeleInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />
                    <Button
                        android:id="@+id/minusBallsOuterTele"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/innerPortTeleTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Inner port"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsInnerTele"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/ballsInnerTeleInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/minusBallsInnerTele"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:id="@+id/ballsAcquiredTeleTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Power Cells Acquired:"
                android:textSize="25dp">
            </TextView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/floorTeleTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Floor"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsFloorTele"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/floorTeleInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />
                    <Button
                        android:id="@+id/minusBallsFloorTele"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/lowChuteTeleTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Low Chute"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsLowChuteTele"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/ballsLowChuteTeleInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />
                    <Button
                        android:id="@+id/minusBallsLowChuteTele"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/highChuteTeleTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="High Chute"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="right">
                    <Button
                        android:id="@+id/plusBallsHighChuteTele"
                        style="@style/t019_plusMinusButton"
                        android:text="+" />
                    <TextView
                        android:id="@+id/ballsHighChuteTeleInput"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />
                    <Button
                        android:id="@+id/minusBallsHighChuteTele"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:id="@+id/controlPanelInteractionTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Control Panel Interaction:"
                android:textSize="25dp">
            </TextView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/performedRotationControlTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Performed Rotation Control"
                    android:layout_weight="1"
                    android:textSize="22dp">
                </TextView>
                <CheckBox
                    android:id="@+id/performedRotationControlCheckBox"
                    style="@style/t019_checkBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="108dp">
                </CheckBox>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/performedPositionControlTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Performed Position Control"
                    android:textSize="22dp">
                </TextView>
                <CheckBox
                    android:id="@+id/performedPositionControlCheckBox"
                    style="@style/t019_checkBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="110dp">
                </CheckBox>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/endGameTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="40dp"
            android:text="End Game:">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/rendezuousTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rendezvous:"
                android:textSize="25dp">
            </TextView>
            <RadioGroup
                android:id="@+id/rendezuousRadioGroup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RadioButton
                    android:id="@+id/noparkRadioButton"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="No Parking"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/parkedRadioButton"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Parked"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/hangedRadioButton"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Hanged"
                    android:textSize="20dp">
                </RadioButton>
            </RadioGroup>
            <TextView
                android:id="@+id/levelingTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Leveling:"
                android:textSize="25dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Large">
            </TextView>
            <RadioGroup
                android:id="@+id/activeLevelingRadioGroup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RadioButton
                    android:id="@+id/noLevelingRadioButton"
                    style="@style/t019_radioButton"
