<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/background"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/assignment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Assignment:"
            android:textSize="40dp">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/txtname"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:text="Scouter:"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="22dp">
            </TextView>
            <TextView
                android:id="@+id/name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Large">
            </TextView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/matchNum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Match Number:"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="22dp">
            </TextView>

            <Button
                android:id="@+id/plusMatch"
                style="@style/t019_plusMinusButton"
                android:layout_width="44dp"
                android:layout_marginLeft="150dp"
                android:text="+" />

            <AutoCompleteTextView
                android:id="@+id/txtMatch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:textStyle="bold" />

            <Button
                android:id="@+id/minusMatch"
                style="@style/t019_plusMinusButton"
                android:layout_weight="0"
                android:text="-" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/logOut"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:shadowColor="#673AB7"
                android:text="Log Out"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="#2196F3" >
            </Button>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/team"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Team"
                android:textSize="22dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Large">
            </TextView>
            <TextView
                android:id="@+id/scoutingTeam"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Large">
            </TextView>
            <TextView
                android:id="@+id/alliance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="0dp"
                android:text="Alliiance Color"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textSize="22dp">
            </TextView>
            <TextView
                android:id="@+id/allianceColor"
                android:layout_width="100dp"
                android:layout_height="match_parent">
            </TextView>
        </LinearLayout>

        <TextView
            android:id="@+id/autonomous"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="40dp"
            android:text="Autonomous:">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/startingPos"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Starting Position:"
                android:textSize="25dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Large">
            </TextView>
            <RadioGroup
                android:id="@+id/startingPosition"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RadioButton
                    android:id="@+id/closest"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Closest to Power Port"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/middle"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Middle"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/farthest"
                    android:layout_width="match_parent"
                    style="@style/t019_radioButton"
                    android:layout_height="wrap_content"
                    android:text="Farthest to Power Port"
                    android:textSize="20dp">
                </RadioButton>
            </RadioGroup>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/startBalls"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Power Cells in robot at start:"
                    android:textSize ="25dp">
                </TextView>
                <RadioGroup
                    android:id="@+id/startingBalls"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <RadioButton
                        android:id="@+id/none"
                        style="@style/t019_radioButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="None"
                        android:textSize="20dp">
                    </RadioButton>
                    <RadioButton
                        android:id="@+id/one"
                        style="@style/t019_radioButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="One"
                        android:textSize="20dp">
                    </RadioButton>
                    <RadioButton
                        android:id="@+id/two"
                        style="@style/t019_radioButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Two"
                        android:textSize="20dp">
                    </RadioButton>
                    <RadioButton
                        android:id="@+id/three"
                        style="@style/t019_radioButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Three"
                        android:textSize="20dp">
                    </RadioButton>
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ballsScoredAuto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Power Cells Scored:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/bottomPortAuto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bottom port"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <Button
                        android:id="@+id/plusBallsBottemAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_marginLeft="170dp"
                        android:text="+" />
                    <AutoCompleteTextView
                        android:id="@+id/txtBallsBottomAuto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/minusBallsBottemAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/outerPortAuto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Outer port"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <Button
                        android:id="@+id/plusBallsOuterAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_marginLeft="190dp"
                        android:text="+" />
                    <AutoCompleteTextView
                        android:id="@+id/txtBallsOuterAuto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/minusBallsOuterAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/innerPortAuto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Inner port"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <Button
                        android:id="@+id/plusBallsInnerAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_marginLeft="195dp"
                        android:text="+" />
                    <AutoCompleteTextView
                        android:id="@+id/txtBallsInnerAuto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/minusBallsInnerAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ballsAcquiredAuto"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Power Cells Acquired:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/floorAuto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Floor"
                        android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:textSize="22dp">
                    </TextView>
                    <Button
                        android:id="@+id/plusBallsFloorAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_marginLeft="240dp"
                        android:text="+" />
                    <AutoCompleteTextView
                        android:id="@+id/txtBallsFloorAuto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textStyle="bold" />

                    <Button
                        android:id="@+id/minusBallsFloorAuto"
                        style="@style/t019_plusMinusButton"
                        android:layout_weight="0"
                        android:text="-" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/crossesLine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Cross Initiation Line:"
                    android:textSize="25dp">
                </TextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/txtCrossesLine"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Left Line at some point"
                        android:textSize="22dp">
                    </TextView>
                    <CheckBox
                        android:id="@+id/crossesLineCheckBox"
                        style="@style/t019_checkBox"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="150dp">
                    </CheckBox>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/teleoperated"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Teleoperated:"
            android:textSize="40dp">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/ballsScoredTele"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Power Cells Scored:"
                android:textSize="25dp">
            </TextView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/bottomPortTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Bottom port"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <Button
                    android:id="@+id/plusBallsBottemTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_marginLeft="170dp"
                    android:text="+" />
                <AutoCompleteTextView
                    android:id="@+id/txtBallsBottemTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/minusBallsBottemTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_weight="0"
                    android:text="-" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/outerPortTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Outer port"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <Button
                    android:id="@+id/plusBallsOuterTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_marginLeft="190dp"
                    android:text="+" />
                <AutoCompleteTextView
                    android:id="@+id/txtBallsOuterTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/minusBallsOuterTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_weight="0"
                    android:text="-" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/innerPortTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Inner port"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <Button
                    android:id="@+id/plusBallsInnerTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_marginLeft="195dp"
                    android:text="+" />
                <AutoCompleteTextView
                    android:id="@+id/txtBallsInnerTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/minusBallsInnerTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_weight="0"
                    android:text="-" />
            </LinearLayout>
            <TextView
                android:id="@+id/ballsAcquiredTele"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Power Cells Acquired:"
                android:textSize="25dp">
            </TextView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/floorTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Floor"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <Button
                    android:id="@+id/plusBallsFloorTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_marginLeft="240dp"
                    android:text="+" />
                <AutoCompleteTextView
                    android:id="@+id/txtFloorTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/minusBallsFloorTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_weight="0"
                    android:text="-" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/lowChuteTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Low Chute"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <Button
                    android:id="@+id/plusBallsLowChuteTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_marginLeft="185dp"
                    android:text="+" />
                <AutoCompleteTextView
                    android:id="@+id/txtBallsLowChuteTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/minusBallsLowChuteTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_weight="0"
                    android:text="-" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/highChuteTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="High Chute"
                    android:textAppearance="@style/TextAppearance.AppCompat.Large"
                    android:textSize="22dp">
                </TextView>
                <Button
                    android:id="@+id/plusBallsHighChuteTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_marginLeft="182dp"
                    android:text="+" />
                <AutoCompleteTextView
                    android:id="@+id/txtBallsHighChuteTele"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/minusBallsHighChuteTele"
                    style="@style/t019_plusMinusButton"
                    android:layout_weight="0"
                    android:text="-" />
            </LinearLayout>
            <TextView
                android:id="@+id/controlPanelInteraction"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Control Panel Interaction:"
                android:textSize="25dp">
            </TextView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/performedRotationControl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Performed Rotation Control"
                    android:textSize="22dp">
                </TextView>
                <CheckBox
                    android:id="@+id/performedRotationControlCheckBox"
                    style="@style/t019_checkBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="108dp">
                </CheckBox>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/performedPositoinControl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Performed Position Control"
                    android:textSize="22dp">
                </TextView>
                <CheckBox
                    android:id="@+id/performedPositionControlCheckBox"
                    style="@style/t019_checkBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="110dp">
                </CheckBox>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/endGame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="40dp"
            android:text="End Game:">
        </TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/rendezuous"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rendezvous:"
                android:textSize="25dp">
            </TextView>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/hanging"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Hanging"
                    android:textSize="22dp">
                </TextView>
                <CheckBox
                    style="@style/t019_checkBox"
                    android:id="@+id/hangingCheckBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="300dp">
                </CheckBox>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/parked"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Parked"
                    android:textSize="22dp">
                </TextView>
                <CheckBox
                    android:id="@+id/parkedCheckBox"
                    style="@style/t019_checkBox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="315dp">
                </CheckBox>
            </LinearLayout>
            <TextView
                android:id="@+id/leveling"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Leveling:"
                android:textSize="25dp"
                android:textAppearance="@style/TextAppearance.AppCompat.Large">
            </TextView>
            <RadioGroup
                android:id="@+id/level"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RadioButton
                    android:id="@+id/noLeveling"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="No leveling"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/triedToLevel"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Tried to level"
                    android:textSize="20dp">
                </RadioButton>
                <RadioButton
                    android:id="@+id/successfullyLeveled"
                    style="@style/t019_radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Successfully leveled"
                    android:textSize="20dp">
                </RadioButton>
            </RadioGroup>
        </LinearLayout>

        <Button
            android:id="@+id/Save"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_marginTop="275dp"
            android:shadowColor="#673AB7"
            android:text="Save"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="#2196F3"/>
    </LinearLayout>
</ScrollView>